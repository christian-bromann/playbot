{
   "commands": [{
      "namespace": "app",
      "action": "create",
      "description": "Create App",
      "options": {
         "required": [
            {
               "key": "title",
               "description": "App's title"
            }, {
               "key": "apk",
               "description": "File path for app's APK"
            }, {
               "key":"subtext",
               "description":"App's description"
            }, {
               "key":"app_type",
               "description":"App's application type",
               "values": ["APPLICATION", "GAME"]
            }, {
               "key":"category",
               "description":"App's category",
               "values": ["GAME_ACTION", "GAME_ADVENTURE", "GAME_ARCADE", 
                  "GAME_BOARD", "GAME_CARD", "GAME_CASINO", "GAME_CASUAL", 
                  "GAME_EDUCATIONAL", "GAME_FAMILY", "GAME_MUSIC", "GAME_PUZZLE", 
                  "GAME_RACING", "GAME_ROLE_PLAYING", "GAME_SIMULATION", 
                  "GAME_SPORTS", "GAME_STRATEGY", "GAME_TRIVIA", "GAME_WORD",
                  "BOOKS_AND_REFERENCE", "BUSINESS", "COMICS", "COMMUNICATION", 
                  "EDUCATION", "ENTERTAINMENT", "FINANCE", "HEALTH_AND_FITNESS", 
                  "LIBRARIES_AND_DEMO", "LIFESTYLE", "MEDIA_AND_VIDEO", 
                  "MEDICAL", "MUSIC_AND_AUDIO", "NEWS_AND_MAGAZINES", "PERSONALIZATION", 
                  "PHOTOGRAPHY", "PRODUCTIVITY", "SHOPPING", "SOCIAL", 
                  "SPORTS", "TOOLS", "TRANSPORTATION", "TRAVEL_AND_LOCAL", "WEATHER"]
            }, {
               "key":"rating",
               "description":"App's content rating",
               "values": ["MATURE", "TEEN", "PRE_TEEN", "SUITABLE_FOR_ALL"]
            }, { 
               "key":"website",
               "description":"Contact Info website"
            }, {
               "key":"public_email",
               "description":"Contact Info email"
            }, {
               "key":"hi_res",
               "description":"File path for app's hi-res icon"
            }, {
               "key":"screenshots_phone",
               "description":"Comma-separated string of paths for screenshots of app on phones"
            }, {
               "key":"screenshots_7",
               "description":"Comma-separated string of paths for screenshots of app on 7-inch devices"
            }, {
               "key":"screenshots_10",
               "description":"Comma-separated string of paths for screenshots of app on 10-inch devices"
            }, {
               "key":"locations",
               "description":"List of countries for app to be distributed in (SELECT ALL COUNTRIES)"
            }, {
               "key":"content_guidelines",
               "description":"Agreement that app meets Android Content Guidelines"
            }, {
               "key":"us_export_laws",
               "description":"Acknowledgement that app may be subject to US export laws"
            }
         ],
         "optional": [
            {
               "key": "language",
               "description": "App's default language",
               "default": "en-US"
            }, {
               "key":"privacy",
               "description":"Privacy policy URL for app",
               "default": ""
            }, {
               "key":"promo",
               "description":"App's promotional text",
               "default": ""
            }, {
               "key": "feat_graphic",
               "description": "File path for app's feature graphic",
               "default": ""
            }, {
               "key": "promo_graphic",
               "description": "File path for app's promo graphic",
               "default": ""
            }, {
               "key": "promo_vid",
               "description": "URL for app's promo video",
               "default": ""
            }, {
               "key": "phone",
               "description": "Contact Info phone",
               "default": ""
            }, {
               "key": "marketing_opt_out",
               "description": "No promotion of application except in Google Play and Google-owned online/mobile properties",
               "default": false
            }, {
               "key": "education",
               "description": "Include app in Google Play for Education",
               "default": false
            }, {
               "key": "education_ads",
               "description": "App has adds - only necessary if opted-in to Google Play for Education",
               "default": false
            }, {
